<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <connectionStrings>
      <add name="Logger" connectionString="Data Source=127.0.0.1;Initial Catalog=LOG;Integrated Security=True" providerName="System.Data.SqlClient" />
    </connectionStrings>  
    <appSettings>
      <add key="LogFileDirectory" value="C:\Log\"/>
      <add key="LogFileName" value="Logger"/>
      <add key="LogFileNameDateFormat" value="yyyyMMdd"/>
      <add key="LogFileLineFormat" value="{0}, Severity: {1}, Message: {2}"/>
    </appSettings>  
</configuration>